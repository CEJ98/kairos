    "test:integration": "vitest run tests/integration/",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:security": "vitest run tests/security/",
    "test:performance": "lighthouse --output=html --output-path=./lighthouse-report.html http://localhost:3000",
    "test:navigation": "node tests/navigation.test.js",
    "test:links": "node scripts/link-checker.js",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:security && npm run test:e2e",
    "verify:final": "node scripts/final-verification.js",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:seed": "tsx prisma/seed.ts",
    "db:studio": "prisma studio",
    "stripe:listen": "stripe listen --forward-to localhost:3000/api/stripe/webhooks",
    "deploy:setup": "node scripts/setup-vercel.js",
    "deploy:preview": "vercel",
    "deploy:production": "vercel --prod",
    "deploy:auto": "node scripts/deploy-vercel.js",
    "deploy:verify": "node scripts/verify-deployment.js",
    "integration:verify": "node scripts/verify-integration.js",
    "github:setup": "node scripts/github-vercel-integration.js",
    "github:guide": "node scripts/connect-github.js",
    "github:connect": "node scripts/github-connect.js",
    "github:verify": "node scripts/verify-github-connection.js",
    "vercel:link": "vercel link",
    "vercel:env": "vercel env ls",
    "vercel:logs": "vercel logs",
    "vercel:domains": "vercel domains ls"
